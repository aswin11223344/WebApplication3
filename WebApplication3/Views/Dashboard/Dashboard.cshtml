<section style=" margin-top:120px;height:800px;">
    <center>
        <div class="card w-75" style="margin-top:70px;background-color:white">
            <h3 class="card-title" style="margin-top:20px;">@ViewBag.Name 's Profile</h3>
            <center>

                <img src="~/uploads/@ViewBag.imagename" class="card-img-top w-25" style="height:500;width:300px; border-radius:25%"; alt="hello">
            </center>
            <h6 style="margin-top:20px">Name:@ViewBag.Name</h6>
            <h6>Age:@ViewBag.Age</h6>
       
            <div class="card-body">
            </div>
            <ul class="list-group list-group-flush">
                <li class="list-group-item"><b>Phone Number:</b>  @ViewBag.PhoneNumber</li>
                <li class="list-group-item"><b>Email:</b> @ViewBag.Email</li>
                <li class="list-group-item"><b>Country:</b> @ViewBag.Country &nbsp;&nbsp;  <b>State:</b> @ViewBag.State &nbsp;&nbsp;  <b>City:</b> @ViewBag.City</li>
            
            </ul>
            <div class="card-body">
               

            </div>
        </div>
    </center>

</section>
